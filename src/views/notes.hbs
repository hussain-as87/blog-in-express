{{> header }}
    <h1>{{ title }}</h1>
    <p>{{ message}}</p>
    <div class="container">
        <table class="table table-hover">
        <thead>
            <tr>
                <th>id</th>
                <th>text</th>
                <th>action</th>
            </tr>
        </thead>
        <tbody>   
             {{#each notes }}
            <tr>
                <td>{{id}}</td>
                <td>{{text}}</td>
                <td><a href="http://localhost:3000/notes/delete/{{id}}" onclick="return confirm('are you sure?')" class="btn btn-danger">deletd</a></td>
            </tr>
            {{/each }}
        </tbody>

    </table></div>
    
    
